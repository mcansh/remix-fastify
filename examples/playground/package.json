{
  "name": "playground",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix build",
    "dev": "remix dev --manual -c \"node --watch-path ./server.js --enable-source-maps ./server.js\"",
    "start": "node --enable-source-maps ./server.js",
    "lint": "eslint --ignore-path .gitignore --no-error-on-unmatched-pattern --cache --cache-location node_modules/.cache/eslint --fix .",
    "format": "prettier --ignore-path .gitignore --ignore-unknown --cache --cache-location node_modules/.cache/prettiercache --write .",
    "typecheck": "tsc",
    "validate": "run-p lint format typecheck"
  },
  "dependencies": {
    "@fastify/early-hints": "^1.0.1",
    "@fastify/static": "^7.0.1",
    "@mcansh/remix-fastify": "workspace:*",
    "@remix-run/node": "*",
    "@remix-run/react": "*",
    "fastify": "^4.26.1",
    "isbot": "^5.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "*",
    "@remix-run/eslint-config": "*",
    "@types/react": "^18.2.58",
    "@types/react-dom": "^18.2.19",
    "chokidar": "^3.6.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "eslint": "^8.56.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18"
  }
}
