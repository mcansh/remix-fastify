    "@remix-run/react": "^2.16.4",
    "chalk": "^5.4.1",
    "cross-env": "^7.0.3",
    "fastify": "^5.2.2",
    "get-port": "^7.1.0",
    "isbot": "^5.1.25",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.16.4",
    "@remix-run/eslint-config": "^2.16.4",
    "@total-typescript/tsconfig": "^1.0.4",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "chokidar": "^4.0.3",
    "eslint": "^8.57.0",
