{
  "include": ["remix.env.d.ts", "**/*.ts", "**/*.tsx"],
  "extends": "@total-typescript/tsconfig/bundler/dom/app",
  "compilerOptions": {
    "types": ["@react-router/node", "vite/client"],
    "jsx": "react-jsx",
    "forceConsistentCasingInFileNames": true,
    "baseUrl": ".",
    "paths": {
      "~/*": ["./app/*"]
    },
    "noEmit": true
  }
}
