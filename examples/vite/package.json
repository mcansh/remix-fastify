{
  "name": "vite-remix",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build && tsc --project ./tsconfig.server.json",
    "dev": "cross-env NODE_ENV=development tsx --watch-path ./server/index.ts ./server/index.ts",
    "lint": "eslint --ignore-path .gitignore --no-error-on-unmatched-pattern --cache --cache-location node_modules/.cache/eslint --fix .",
    "format": "prettier --ignore-path .gitignore --ignore-unknown --cache --cache-location node_modules/.cache/prettiercache --write .",
    "start": "cross-env NODE_ENV=production node ./server/index.js",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@fastify/middie": "^9.0.2",
    "@fastify/static": "^8.0.3",
    "@mcansh/remix-fastify": "4.0.4",
    "@remix-run/node": "*",
    "@remix-run/react": "*",
    "@remix-run/server-runtime": "*",
    "chalk": "^5.3.0",
    "cross-env": "^7.0.3",
    "fastify": "^5.1.0",
    "get-port": "^7.1.0",
    "isbot": "^5.1.17",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@remix-run/dev": "*",
    "@remix-run/eslint-config": "*",
    "@total-typescript/tsconfig": "^1.0.4",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/source-map-support": "^0.5.10",
    "eslint": "^8.57.0",
    "prettier": "^3.3.3",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vite": "^5.4.11",
    "vite-tsconfig-paths": "^5.1.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
