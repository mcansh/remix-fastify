{
  "name": "vite-remix",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "vite build && vite build --ssr",
    "dev": "cross-env NODE_ENV=development node --watch-path ./server.js --enable-source-maps ./server.js",
    "lint": "eslint --ignore-path .gitignore --no-error-on-unmatched-pattern --cache --cache-location node_modules/.cache/eslint --fix .",
    "start": "cross-env NODE_ENV=production node --enable-source-maps ./server.js",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@fastify/middie": "^8.3.0",
    "@fastify/static": "^7.0.1",
    "@mcansh/remix-fastify": "3.2.2",
    "@remix-run/node": "*",
    "@remix-run/react": "*",
    "cross-env": "^7.0.3",
    "fastify": "^4.26.1",
    "isbot": "^5.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "*",
    "@remix-run/eslint-config": "*",
    "@types/react": "^18.2.57",
    "@types/react-dom": "^18.2.19",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3",
    "vite": "^5.1.3",
    "vite-tsconfig-paths": "^4.3.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
