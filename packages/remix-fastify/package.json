{
  "name": "@mcansh/remix-fastify",
  "version": "3.4.1",
  "description": "Fastify server request handler for Remix",
  "repository": "mcansh/remix-fastify",
  "license": "MIT",
  "author": "Logan McAnsh <logan@mcan.sh> (https://mcan.sh)",
  "bugs": {
    "url": "https://github.com/mcansh/remix-fastify/issues"
  },
  "keywords": [
    "remix",
    "remix-run",
    "fastify"
  ],
  "funding": [
    {
      "type": "github",
      "url": "https://github.com/sponsors/mcansh"
    }
  ],
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "source": "./src/index.ts",
  "types": "./dist/index.d.cts",
  "files": [
    "dist",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "prepublishOnly": "npm run build && cp ../../LICENSE LICENSE && publint && attw $(npm pack)",
    "typecheck": "tsc",
    "dev": "tsup --watch",
    "build": "tsup",
    "test": "vitest",
    "validate": "run-p typecheck test"
  },
  "dependencies": {
    "@fastify/middie": "^9.0.2",
    "@fastify/static": "^8.0.2",
    "fastify-plugin": "^5.0.1",
    "pretty-cache-header": "^1.0.0"
  },
  "devDependencies": {
    "@remix-run/node": "^2.13.1",
    "@types/node": "^22.7.7",
    "@typescript/lib-dom": "npm:@types/web@^0.0.174",
    "fastify": "^5.0.0",
    "node-mocks-http": "^1.16.1",
    "typescript": "^5.6.3",
    "vite": "^5.4.9"
  },
  "peerDependencies": {
    "@remix-run/node": "^2.0.0",
    "fastify": "^3.29.0 || ^4.0.0 || ^5.0.0",
    "react-router": "*",
    "vite": "^5.0.0"
  },
  "peerDependenciesMeta": {
    "@remix-run/node": {
      "optional": true
    },
    "react-router": {
      "optional": true
    },
    "vite": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
