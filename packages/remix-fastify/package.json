{
  "name": "@mcansh/remix-fastify",
  "version": "3.0.0",
  "description": "Fastify server request handler for Remix",
  "repository": "mcansh/remix-fastify",
  "license": "MIT",
  "author": "Logan McAnsh <logan@remix.run> (https://remix.run)",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": {
        "require": "./dist/index.d.cts",
        "import": "./dist/index.d.ts",
        "default": "./dist/index.d.cts"
      },
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "default": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "source": "./src/index.ts",
  "types": "./dist/index.d.cts",
  "files": [
    "dist",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "prepublishOnly": "npm run build && publint",
    "typecheck": "tsc",
    "dev": "tsup --watch",
    "build": "tsup",
    "test": "vitest",
    "validate": "run-p typecheck test"
  },
  "dependencies": {
    "@fastify/static": "^6.11.2",
    "@remix-run/router": "^1.9.0",
    "fastify-plugin": "^4.5.1",
    "glob": "^10.3.4"
  },
  "devDependencies": {
    "@fastify/early-hints": "^1.0.1",
    "@remix-run/node": "^2.0.0",
    "@remix-run/server-runtime": "^2.0.0",
    "@types/glob": "^8.1.0",
    "@types/node": "^20.6.2",
    "@typescript/lib-dom": "npm:@types/web@^0.0.114",
    "fastify": "^4.23.2",
    "node-mocks-http": "^1.13.0",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "@fastify/early-hints": "^1.0.1",
    "@remix-run/node": "^2.0.0",
    "fastify": "^3.29.0 || ^4.0.0"
  },
  "peerDependenciesMeta": {
    "@fastify/early-hints": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
